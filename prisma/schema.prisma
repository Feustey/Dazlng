generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model History {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  date      DateTime @default(now())
  price     Float
  volume    Float
  marketCap Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
} 