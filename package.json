{
  "name": "dazenode",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:with-validation": "npm run validate-translations && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "validate-translations": "tsx scripts/validate-translations.ts",
    "i18n:extract": "tsx scripts/extract-translations.ts",
    "i18n:validate": "tsx scripts/validate-translations.ts",
    "i18n:migrate": "tsx scripts/migrate-to-i18n.ts",
    "i18n:report": "tsx scripts/i18n-coverage-report.ts",
    "i18n:check": "npm run i18n:validate && npm run i18n:report",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:dazflow": "tsx scripts/test-dazflow-endpoints.ts",
    "test:node-connection": "tsx scripts/test-node-connection.ts",
    "test:supabase-diagnostic": "tsx scripts/test-supabase-instances.ts",
    "verify-migration": "tsx scripts/verify-post-migration.ts",
    "test-apis": "tsx scripts/test-apis-post-migration.ts",
    "health-check": "tsx scripts/quick-health-check.ts",
    "check-migration": "tsx scripts/simple-migration-check.ts",
    "create-admin": "tsx scripts/create-dev-admin.ts",
    "verify-all": "npm run verify-migration && npm run test-apis",
    "cleanup-obsolete": "tsx scripts/cleanup-obsolete-files.ts",
    "cleanup:lightning": "tsx scripts/cleanup-lightning-fallback.ts",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "test:vitals": "npx lighthouse http://localhost:3000 --view --only-categories=performance",
    "perf:audit": "npm run analyze:bundle && npm run test:vitals",
    "cache:clear": "rm -rf .next/cache",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "optimize": "npm run cache:clear && npm run build:analyze",
    "generate-jwt": "tsx scripts/generate-jwt.ts",
    "test:jwt": "tsx scripts/test-jwt-auth.ts",
    "setup-env": "node scripts/setup-env.js",
    "test:user-endpoints": "node scripts/test-user-endpoints.js",
    "supabase:migrate": "tsx scripts/apply-supabase-migration.ts",
    "supabase:test": "tsx scripts/test-supabase-migration.ts",
    "supabase:validate": "npm run supabase:test && npm run supabase:migrate",
    "test:cross-domain-session": "tsx scripts/test-cross-domain-session.ts",
    "test:dazno-api": "tsx scripts/test-dazno-api.ts",
    "check:dazno-config": "tsx scripts/check-dazno-config.ts",
    "test:mcp-fix": "tsx scripts/test-mcp-fix.ts",
    "test:mcp-quick": "tsx scripts/quick-mcp-test.ts",
    "deploy:mcp-fix": "chmod +x scripts/execute-deployment.sh && ./scripts/execute-deployment.sh",
    "deploy:mcp-safe": "chmod +x scripts/deploy-with-test.sh && ./scripts/deploy-with-test.sh",
    "deploy:mcp-final": "chmod +x scripts/final-deploy.sh && ./scripts/final-deploy.sh",

    "optimize:css": "npm run build && npm run analyze:bundle",
    "optimize:all": "npm run cache:clear && npm run build",
    "cleanup:deps": "node scripts/cleanup-dependencies.js",
    "optimize:full": "npm run cleanup:deps && npm run cache:clear && npm run build",
    "perf:report": "npm run build && npx lighthouse http://localhost:3000 --view --only-categories=performance --output=json --output-path=./lighthouse-report.json"
  },
